  "_meta": {
    "models": {
      "projectStatusReports": "Summarizes the health of each project with on-track, at-risk, or overdue indicators based on deadlines and progress.",
      "workloadReports": "Shows team member capacity versus current workload to identify who is overloaded or has availability.",
      "taskAgingReports": "Tracks tasks that have been sitting in a status for too long, highlighting potential bottlenecks.",
      "blockerReports": "Lists all blocked work items with reasons and duration to surface impediments.",
      "timeBudgetReports": "Compares hours logged versus estimated hours and tracks budget burn rate against allocated budget.",
      "approvalsVelocityReports": "Measures how long items wait in review before approval or rejection.",
      "agentActivityReports": "Tracks AI agent runs, outputs generated, approval rates, and operational costs.",
      "aiInsights": "AI-generated recommendations, anomaly detections, and alerts that surface issues needing attention.",
      "projects": "A collection of work being done for a party, containing multiple work orders.",
      "teamMembers": "A team member (human or AI agent) that can be assigned to work.",
      "tasks": "Individual action items that make up a work order.",
      "approvals": "Requests for human review and sign-off on agent-generated work."
    },
    "relationships": [
      "Project Status Reports reference Projects",
      "Workload Reports reference Team Members",
      "Task Aging Reports reference Tasks",
      "Blocker Reports can reference Projects, Work Orders, or Tasks",
      "Time & Budget Reports reference Projects",
      "Approvals Velocity Reports reference Approvals",
      "Agent Activity Reports reference AI Agents and their outputs",
      "AI Insights can relate to any report type or entity"
    ]
  },
