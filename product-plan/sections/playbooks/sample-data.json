    {
      "id": "sop-002",
      "type": "sop",
      "name": "Website Launch Procedure",
      "description": "Step-by-step process for deploying a website from staging to production",
      "triggerConditions": "When website development is complete and QA has passed",
      "steps": [
        {
          "id": "step-1",
          "order": 1,
          "title": "Run final QA checks",
          "description": "Complete QA checklist on staging environment, verify all functionality",
          "evidenceRequired": "document",
          "evidenceDescription": "Completed QA checklist with all items passed",
          "assignedRole": "QA Lead"
        },
        {
          "id": "step-2",
          "order": 2,
          "title": "Get client approval",
          "description": "Send staging link to client for final review and sign-off",
          "evidenceRequired": "approval",
          "evidenceDescription": "Client approval email or comment",
          "assignedRole": "Project Manager"
        },
        {
          "id": "step-3",
          "order": 3,
          "title": "Backup production",
          "description": "Create full backup of current production environment if replacing existing site",
          "evidenceRequired": "link",
          "evidenceDescription": "Backup confirmation or storage link",
          "assignedRole": "Developer"
        },
        {
          "id": "step-4",
          "order": 4,
          "title": "Deploy to production",
          "description": "Push code to production server, verify DNS and SSL configuration",
          "evidenceRequired": "link",
          "evidenceDescription": "Live production URL",
          "assignedRole": "Developer"
        },
        {
          "id": "step-5",
          "order": 5,
          "title": "Run smoke tests",
          "description": "Test critical paths on production: homepage, key forms, mobile responsiveness",
          "evidenceRequired": "document",
          "evidenceDescription": "Smoke test checklist completed",
          "assignedRole": "QA Lead"
        },
        {
          "id": "step-6",
          "order": 6,
          "title": "Monitor analytics setup",
          "description": "Verify tracking codes firing correctly, confirm data in analytics dashboard",
          "evidenceRequired": "screenshot",
          "evidenceDescription": "Screenshot of analytics confirming data",
          "assignedRole": "Developer"
        },
        {
          "id": "step-7",
          "order": 7,
          "title": "Send launch notification",
          "description": "Email client with live URL and next steps for monitoring and support",
          "evidenceRequired": "approval",
          "evidenceDescription": "Launch notification email sent",
          "assignedRole": "Project Manager"
        }
      ],
      "rolesInvolved": ["Developer", "QA Lead", "Project Manager"],
      "estimatedTimeMinutes": 180,
      "definitionOfDone": "Site is live on production URL, smoke tests pass, client is notified, analytics confirmed working",
