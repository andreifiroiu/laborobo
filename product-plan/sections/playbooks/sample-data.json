    {
      "id": "sop-001",
      "type": "sop",
      "name": "Client Onboarding Process",
      "description": "Complete procedure for onboarding new clients from kickoff to first deliverable",
      "triggerConditions": "When a new client signs a contract or project is won",
      "steps": [
        {
          "id": "step-1",
          "order": 1,
          "title": "Schedule kickoff meeting",
          "description": "Set up initial meeting with client stakeholders within 3 business days of contract signing",
          "evidenceRequired": "calendar-invite",
          "evidenceDescription": "Meeting invite with all stakeholders",
          "assignedRole": "Project Manager"
        },
        {
          "id": "step-2",
          "order": 2,
          "title": "Gather requirements",
          "description": "Complete project brief template with client input during kickoff meeting",
          "evidenceRequired": "document",
          "evidenceDescription": "Completed project brief",
          "assignedRole": "Project Manager"
        },
        {
          "id": "step-3",
          "order": 3,
          "title": "Set up project workspace",
          "description": "Create project in system, add team members, create initial work orders",
          "evidenceRequired": "link",
          "evidenceDescription": "Link to project workspace",
          "assignedRole": "Project Manager"
        },
        {
          "id": "step-4",
          "order": 4,
          "title": "Create project timeline",
          "description": "Build milestone schedule with deliverable dates and client review points",
          "evidenceRequired": "document",
          "evidenceDescription": "Project timeline document",
          "assignedRole": "Project Manager"
        },
        {
          "id": "step-5",
          "order": 5,
          "title": "Send welcome package",
          "description": "Email client with project access, timeline, team contacts, and next steps",
          "evidenceRequired": "approval",
          "evidenceDescription": "Client confirmation email",
          "assignedRole": "Project Manager"
        }
      ],
      "rolesInvolved": ["Project Manager", "Account Manager", "Team Lead"],
      "estimatedTimeMinutes": 240,
      "definitionOfDone": "Client has access to project workspace, timeline is approved, first work order is created and assigned",
